{
    "name": "amazeeio/symfony-amazeeai-configure",
    "description": "Symfony command to configure the amazee.ai provider via email-based authentication.",
    "type": "project",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "keywords": [
        "ai",
        "amazee.io"
    ],
    "require": {
        "php": ">=8.4",
        "symfony/console": "^8.0",
        "symfony/http-client": "^8.0",
        "symfony/process": "^8.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-strict-rules": "^2.0",
        "phpunit/phpunit": "^12.5",
        "symfony/dotenv": "^8.0",
        "friendsofphp/php-cs-fixer": "^3.93",
        "phpstan/phpstan-symfony": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "AmazeeIO\\AmazeeAIConfigure\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AmazeeIO\\AmazeeAIConfigure\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Christophe Jossart",
            "email": "christophe.jossart@amazee.io"
        }
    ]
}
